{
    "name": "friendly-notebook-frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "framer-motion": "^10.16.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^4.11.0",
        "react-markdown": "^9.0.0",
        "react-router-dom": "^6.18.0",
        "react-scripts": "5.0.1",
        "react-toastify": "^11.0.5",
        "remark-gfm": "^4.0.0",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "dev": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "start:backend": "cd backend && npm start",
        "start:all": "powershell -ExecutionPolicy Bypass -File FNBXAI.ps1",
        "start:batch": "START_FNBXAI.bat",
        "start:individual": "START_INDIVIDUAL.bat",
        "start:backend-only": "powershell -ExecutionPolicy Bypass -File START_BACKEND.ps1",
        "start:frontend-only": "powershell -ExecutionPolicy Bypass -File START_FRONTEND.ps1",
        "start:knowledge-only": "powershell -ExecutionPolicy Bypass -File START_KNOWLEDGE.ps1",
        "start:monitor-only": "powershell -ExecutionPolicy Bypass -File START_AI_MONITOR.ps1",
        "start:db-only": "powershell -ExecutionPolicy Bypass -File START_DB_MANAGER.ps1",
        "seed:knowledge": "cd backend && npm run seed-specialized",
        "test:ai": "cd backend && node testDashboardAI.js",
        "test:llm": "cd backend && node testLLMSetup.js"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
